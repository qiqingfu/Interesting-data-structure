{
  "name": "interesting-data-structure",
  "version": "1.0.0",
  "description": "Interesting data structure",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server",
    "build": "webpack --config webpack.config.js",
    "lint": "eslint src test",
    "test": "npm run lint && jest",
    "coverage": "jest --coverage",
    "commit": "git-cz"
